<h1>Members</h1>

<p>
  <a href="/members/new">New Member</a>
</p>

<table>
  <tr>
    <th>Name</th>
    <th>age</th>
    <th>membership</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.name %></a></td>
    <td><%= member.age %></td>
    <td><%= member.membership() %></td>
  </tr>
  <% end %>
</table>
